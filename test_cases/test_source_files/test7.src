function POLYNOMIAL::evaluate(x: float) => float
{
  return (0);
}

function QUADRATIC::evaluate(x: float) => float
{
  localvar result: float;
  result = a;
  result = result * x + b;
  result = result * x + c;
  return (result);
}

function QUADRATIC::constructor(A: float, B: float, C: float)
{
  self.a = A;
  self.b = B;
  self.c = C;
}
